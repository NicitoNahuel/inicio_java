package gcgcf;

public class Program {
	public static void main(String[] args) {
		int numero = 0 ;
		for (int i=0; i<100; i++)
        {
            if (numero % 2 == 0)
            {System.out.println (numero + " es par");}
            else 
            {System.out.println(numero);}
            numero++;
        }
	}
}
